{
  "name": "sitemap-generator-webpack-plugin",
  "description": "Webpack plugin to generate a sitemap.",
  "license": "MIT",
  "version": "0.1.1",
  "author": "JangHyeok Kim",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d0orHyeok/sitemap-generator-webpack-plugin.git"
  },
  "homepage": "https://github.com/d0orHyeok/sitemap-generator-webpack-plugin",
  "bugs": "https://github.com/d0orHyeok/sitemap-generator-webpack-plugin",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">= 14.15.0"
  },
  "keywords": [
    "webpack",
    "plugin",
    "sitemap",
    "webpack-plugin",
    "sitemap-generator-webpack-plugin"
  ],
  "files": [
    "dist/",
    "types/",
    "LICENSE",
    "CHANGELOG.md",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "npm run build",
    "clean": "rimraf dist types",
    "prebuild": "npm run clean",
    "build": "npm-run-all -p \"build:**\"",
    "build:types": "tsc && prettier \"types/**/*.ts\" --write",
    "build:code": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "lint:prettier": "prettier --list-different .",
    "lint:js": "eslint --cache .",
    "lint:types": "tsc --pretty --noEmit",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "test": "npm run test:only",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "peerDependencies": {
    "webpack": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "babel-jest": "^29.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "minimatch": "^9.0.3",
    "schema-utils": "^4.2.0"
  }
}
