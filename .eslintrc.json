{
  "parser": "@babel/eslint-parser",
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:eslint-plugin/recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-unresolved": "off"
  }
}
